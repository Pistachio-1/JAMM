  "private": true,
  "dependencies": {
    "ajv": "^6.0.0",
    "ajv-keywords": "^3.2.0",
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "mysql2": "^1.5.3",
    "node-sass-chokidar": "^1.3.0",
    "npm": "^6.2.0",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
